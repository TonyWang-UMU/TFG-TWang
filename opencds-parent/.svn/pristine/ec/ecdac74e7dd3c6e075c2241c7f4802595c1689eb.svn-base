		<artifactId>opencds-vmr-1_0-schema</artifactId>
		<version>${project.version}</version>
  	</dependency>
  	<dependency>
  		<groupId>org.mod4j.org.apache.commons</groupId>
  		<artifactId>logging</artifactId>
  		<version>1.0.4</version>
  	</dependency>
