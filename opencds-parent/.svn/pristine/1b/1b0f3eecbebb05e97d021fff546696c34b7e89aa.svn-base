package org.opencds.common.structures;

/**
 * <p>ObjectPair contains a single pair of objects.</p>
 * 
 *  @author Kensaku Kawamoto
 * @version 1.00
 */
public class ObjectPair extends Object
{
    protected Object myObject1;
    protected Object myObject2;

    public ObjectPair()
    {
    }

    public ObjectPair(Object object1, Object object2)
    {
        this.myObject1 = object1;
        this.myObject2 = object2;
    }

    // setter functions
    public void setObject1(Object object1)
    {
        this.myObject1 = object1;
    }

    public void setObject2(Object object2)
    {
        this.myObject2 = object2;
    }

    // getter functions
    public Object getObject1()
    {
        return myObject1;
    }

    public Object getObject2()
    {
        return myObject2;
    }
}